<style>
            div, input {
                border-radius: 10px;
            }
        </style>

<h3> Listagem de Marcações</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Nome do Paciente</th>
            <th>Nome do Exame</th>
            <th>Data da Marcação</th>            
            <th>Validade</th>            
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td id="nomePaciente">{{this._idPaciente.nome}}</td>
            <td id="nomeExame">{{this._idExame.nome}}</td>
            <td id="dataExame">{{this.dataExameConvertido}}</td>            
            <td id="dataExame">{{this._idExame.validade}} dias</td>
        </tr>
        {{/each}}
    </tbody>
</table>

<a href="/">
    <button class="btn btn-warning">Voltar</button>
</a>

